{
  "name": "bingo",
  "version": "1.0.0",
  "description": "package.json for keeping track of node dependencies for Bingo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shivam-kapila/Bingo.git"
  },
  "scripts": {
    "build:dev": "webpack --progress --colors",
    "build:prod": "webpack -p --env production --progress --colors && npm run build:less",
    "build:less": "./node_modules/less/bin/lessc --clean-css ./webserver/static/css/main.less > ./webserver/static/css/main.css",
    "build:types": "tsc --emitDeclarationOny",
    "build": "npm run build:dev",
    "format": "eslint ./static/js/src --ext .js,jsx,ts,tsx --fix --quiet",
    "format:ci": "eslint ./static/js/src --ext .js,jsx,ts,tsx -o eslint.xml --format checkstyle",
    "type-check": "tsc --noEmit"
  },
  "author": "Shivam Kapila",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "babel-runtime": "^6.26.0",
    "clean-css": "^4.2.1",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-bs-notifier": "^5.0.0",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.9.0",
    "@fortawesome/fontawesome-common-types": "^0.2.28",
    "@types/clean-css": "^4.2.1",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "@types/react-loader-spinner": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fork-ts-checker-webpack-plugin": "^6.0.5",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
